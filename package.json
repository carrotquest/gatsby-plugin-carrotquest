    "carrot quest",
    "carrotquest.io",
    "live-chat",
    "chatbot"
  ],
  "dependencies": {
    "@babel/runtime": "^7.20.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "babel-preset-gatsby-package": "^3.3.0",
    "cross-env": "^7.0.3"
  },
  "peerDependencies": {
    "@babel/core": "^7.11.6",
    "gatsby": ">4.0.0"
  }
}
