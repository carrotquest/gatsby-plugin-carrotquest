{
  "name": "gatsby-plugin-carrotquest",
  "version": "0.4.2",
  "description": "Gatsby плагин для Carrot quest",
  "author": "Nikolay Abdullin <addonion@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/carrotquest/gatsby-plugin-carrotquest"
  },
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore \"**/__tests__\"",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore \"**/__tests__\""
